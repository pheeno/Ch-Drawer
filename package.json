{
  "name": "ch-drawer",
  "version": "0.1.0",
  "private": false,
  "main": "dist/ch-drawer.common.js",
  "keyword": [
    "vue drawer",
    "drawer",
    "sidebar",
    "vue",
    "sider",
    "web drawer"
  ],
  "author": "Johnny 'Boggie' Ness",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:pheeno/Ch-Drawer.git"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name ch-drawer src/packages/index.js",
    "build:analyz": "vue-cli-service build --mode analyz --target lib --name ch-drawer src/packages/index.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test": "vue-cli-service test:unit",
    "style-reset": "rimraf ./src/packages/style.css",
    "style-output": "stylus ./src/packages/style.styl -o ./src/packages/style.css",
    "style-check": "npm run style-reset & npm run style-output",
    "docs:dev": "sudo vuepress dev docs",
    "docs:build": "sudo vuepress build docs"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
